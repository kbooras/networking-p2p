
public class Message {
		int length;
		int type;
		String payload;
		
		public Message()
		{
			length = -1;
			type = -1;
			payload = null;
		}
		
		public Message(int length, int type, String payload)
		{
			this.length = length;
			this.type = type;
			this.payload = payload;
		}
		
		public int getLength()
		{
			return length;
		}
		
		public void setLength(int length)
		{
			this.length = length;
		}
		
		public int getType()
		{
			return type;
		}
		
		public void setType(int type)
		{
			this.type = type;
		}
		
		
}
