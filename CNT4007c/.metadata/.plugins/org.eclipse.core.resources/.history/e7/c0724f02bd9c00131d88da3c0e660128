
public class HandshakeMessage {
	String header;
	String zeroBits;
	int peerID;
	
	public HandshakeMessage()
	{
		this.header = null;
		this.zeroBits = null;
		peerID = -1;
	}
	public HandshakeMessage(String header, String zeroBits, int peerID)
	{
		this.header = header;
		this.zeroBits = zeroBits;
		this.peerID = peerID;
	}
}
